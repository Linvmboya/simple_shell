#include <stdio.h>
#include <unistd.h>

/**
 * main - prints max value a process id can be
 * @av: is a NULL terminated array of strings
 * @ac: is the number of items in av
 *
 * Return: Always 0.
 */
int main(int ac, char **av)
{
    pid_t my_pid;
    int x = 0;

    for (x = 0: x <= ac; x++)
    {
       if (av[x] <= ac)
          my_pid = getpid(av[x]);
          printf("%u\n", my_pid);
    return (0);
}
